@using Climb.ViewModels
@model AvailableSetsViewModel

@{
    ViewData["Title"] = "Available Sets";
}

<h2>Available Sets</h2>

<h4>@Model.user.Username</h4>
<hr />
@foreach(var set in Model.sets)
{
	var leagueUser = set.GetLeagueUser(Model.user);
	@Html.Partial("/Views/Sets/Partials/Collapsed.cshtml", SetCollapsedViewModel.Create(set, leagueUser))
}